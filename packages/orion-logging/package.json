{
  "name": "@orion/logging",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./transports/console": {
      "import": "./src/transports/console.ts",
      "types": "./src/transports/console.ts"
    },
    "./transports/base": {
      "import": "./src/transports/base.ts",
      "types": "./src/transports/base.ts"
    },
    "./transports/cloudwatch": {
      "import": "./src/transports/cloudwatch.ts",
      "types": "./src/transports/cloudwatch.ts"
    },
    "./transports/*": "./src/transports/*.ts"
  },
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx tests/basic.test.ts",
    "test": "tsx --test tests/**/*.test.ts",
    "lint": "eslint . --ext .ts",
    "prepack": "pnpm build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.948.0"
  }
}
